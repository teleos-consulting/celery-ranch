[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
# Set your PyPI API token here or use environment variable: TWINE_PASSWORD
# password = pypi-xxxx

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
# Set your TestPyPI API token here or use environment variable: TWINE_PASSWORD
# password = pypi-xxxx